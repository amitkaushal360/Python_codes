#approach 1 using for and if-else
lst=[10,30,40,20,50]
lst1=[]
for i in range(len(lst)):
    if i==0:
        lst1.append(lst[0])
    else:
        lst1.append(lst[i]+lst1[i-1])
print("output using approach1:",lst1)

#approach 2 using only for
list2=[10,20,30,40,50]
new_list2=[]
j=0
for i in range(len(list2)):
	j+=list2[i]
	new_list2.append(j)
print("output using approach2:",new_list2)
