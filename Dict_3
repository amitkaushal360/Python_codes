d={"a":3,"b":2,"c":8,"d":6,"e":7}

#using del function

print("Before deletion of a:",d)
del d["a"]
print("After deletion of a:",d)

#using pop function
print("\nBefore deletion of c:",d)
d.pop("c")
print("After deletion of c:",d)

#using list comprehension
print("\nBefore deletion of b:",d)
new_d={key:val for key,val in d.items() if d.keys()!="b"}
print("After deletion of c:",new_d)
