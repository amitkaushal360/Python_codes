def check_palin_symm(a):
    n=int(len(a)/2)
    if a==a[::-1]:
        print("It's a palindrome")
    elif a[:n]==a[n:]:
        print("It's a symmetrical")
    else:
        print("It's not palindrome or symmetrical ")
check_palin_symm("radar")
check_palin_symm("khokho")
check_palin_symm("radaaar")
