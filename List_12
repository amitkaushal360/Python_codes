list1 = [10, 20, 4, 45, 99]
n=3
list1.sort()
print("N largest elements in List1 are:", list1[-n:])

list2 = [2, 6, 41, 85, 0, 3, 7, 6, 10]
N = 2
final_list = []
for i in range(0, N): 
    max1 = 0
          
    for j in range(len(list2)):     
        if list2[j] > max1:
            max1 = list2[j];
                  
    list2.remove(max1);
    final_list.append(max1)
          
print("N largest elements in List1 are:",final_list)
